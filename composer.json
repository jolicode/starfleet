    "require": {
        "php": "^7.3",
        "ext-json": "*",
        "doctrine/doctrine-bundle": "^1.6.10|^2.0",
        "doctrine/doctrine-migrations-bundle": "^1.3|^2.0",
        "doctrine/orm": "^2.5.11",
        "easycorp/easy-log-handler": "^1.0.7",
        "easycorp/easyadmin-bundle": "^2.3",
        "knpuniversity/oauth2-client-bundle": "^1.15",
        "league/oauth2-google": "^3.0",
        "myclabs/php-enum": "^1.6",
        "php-translation/symfony-bundle": "^0.9",
        "sensio/framework-extra-bundle": "^5.2",
        "stof/doctrine-extensions-bundle": "^1.3",
        "symfony/asset": "v4.4.0-RC1",
        "symfony/console": "v4.4.0-RC1",
        "symfony/contracts": "^1.1",
        "symfony/debug-bundle": "v4.4.0-RC1",
        "symfony/dependency-injection": "v4.4.0-RC1",
        "symfony/dotenv": "v4.4.0-RC1",
        "symfony/flex": "^1.0",
        "symfony/framework-bundle": "v4.4.0-RC1",
        "symfony/http-client": "v4.4.0-RC1",
        "symfony/maker-bundle": "^1.11",
        "symfony/monolog-bundle": "^3.0",
        "symfony/serializer": "v4.4.0-RC1",
        "symfony/stopwatch": "v4.4.0-RC1",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/translation": "v4.4.0-RC1",
        "symfony/twig-bundle": "v4.4.0-RC1",
        "symfony/var-dumper": "v4.4.0-RC1",
        "symfony/web-profiler-bundle": "v4.4.0-RC1",
        "symfony/webpack-encore-bundle": "^1.7",
        "symfony/workflow": "v4.4.0-RC1",
        "symfony/yaml": "v4.4.0-RC1",
        "twig/extensions": "^1.5"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.0",
        "friendsofphp/php-cs-fixer": "^2.9",
        "nelmio/alice": "^3.1",
        "symfony/browser-kit": "v4.4.0-RC1",
        "symfony/phpunit-bridge": "v4.4.0-RC1",
        "symfony/thanks": "^1.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "id": "01C0VZ6G7PR4N92Q4W9XRGN034",
            "allow-contrib": true
        }
    }
}
