    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "JoliCode",
            "email": "starfleet@jolicode.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "alterphp/easyadmin-extension-bundle": "^3.0",
        "doctrine/doctrine-bundle": "^1.6.10|^2.0",
        "doctrine/doctrine-migrations-bundle": "^2.0",
        "doctrine/orm": "^2.5.11",
        "easycorp/easy-log-handler": "^1.0.7",
        "easycorp/easyadmin-bundle": "^2.3",
        "geocoder-php/nominatim-provider": "^5.1",
        "knpuniversity/oauth2-client-bundle": "^2.6.0",
        "league/oauth2-github": "^2.0",
        "league/oauth2-google": "^3.0",
        "myclabs/php-enum": "^1.6",
        "php-http/guzzle6-adapter": "^2.0",
        "php-http/message": "^1.8",
        "php-translation/symfony-bundle": "^0.12.3",
        "phpspec/prophecy": "^1.13.0",
        "sameer-shelavale/php-countries-array": "^1.0",
        "sensio/framework-extra-bundle": "^6.1.1",
        "stof/doctrine-extensions-bundle": "^1.3",
        "symfony/asset": "^5.3",
        "symfony/console": "^5.3",
        "symfony/contracts": "^2.3.1",
        "symfony/debug-bundle": "^5.3",
        "symfony/dependency-injection": "^5.3",
        "symfony/dotenv": "^5.3",
        "symfony/filesystem": "5.3.*",
        "symfony/flex": "^1.0",
        "symfony/form": "5.3.*",
        "symfony/framework-bundle": "^5.3",
        "symfony/http-client": "^5.3",
        "symfony/maker-bundle": "^1.11",
        "symfony/monolog-bundle": "^3.0",
        "symfony/phpunit-bridge": "^5.3",
        "symfony/polyfill-uuid": "^1.20",
        "symfony/process": "5.3.*",
        "symfony/runtime": "5.3.*",
        "symfony/serializer": "^5.3",
        "symfony/stopwatch": "^5.3",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/translation": "^5.3",
        "symfony/twig-bundle": "^5.3",
        "symfony/ux-chartjs": "^1.2",
        "symfony/var-dumper": "^5.3",
        "symfony/web-profiler-bundle": "^5.3",
        "symfony/webpack-encore-bundle": "^1.7",
        "symfony/workflow": "^5.3",
        "symfony/yaml": "^5.3",
        "twig/extensions": "^1.5",
        "twig/extra-bundle": "^3.3",
        "twig/string-extra": "^3.3"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.0",
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpspec/prophecy-phpunit": "^2.0",
        "phpstan/phpstan": "^0.12.63",
        "phpstan/phpstan-doctrine": "^0.12.26",
        "phpstan/phpstan-symfony": "^0.12.12",
        "symfony/browser-kit": "^5.3",
        "symfony/css-selector": "^5.3",
        "symfony/thanks": "^1.0",
        "zenstruck/foundry": "^1.10"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "require": "5.3.*",
            "allow-contrib": true
        },
        "phpstan": {
            "includes": [
                "extension.neon"
            ]
        }
    }
}
